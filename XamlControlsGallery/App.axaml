             xmlns:local="clr-namespace:XamlControlsGallery"
             x:Class="XamlControlsGallery.App" Name="My App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <StyleInclude Source="avares://AvaloniaEdit/AvaloniaEdit.axaml" />
        <StyleInclude Source="/Styles/Styles.axaml" />
        <StyleInclude Source="/Styles/SideBar.axaml" />
        <StyleInclude Source="/Styles/Themes/BaseLight.axaml" />
    </Application.Styles>
</Application>
